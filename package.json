{
  "name": "@iceteachain/sunseed",
  "version": "0.1.0",
  "description": "Compile djs to raw js for deploying on Icetea Blockchain.",
  "main": "index.js",
  "browser": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "test": "jest --coverage",
    "lint": "standard",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TradaTech/sunseed.git"
  },
  "author": "Trada Tech <iceteachain@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TradaTech/sunseed/issues"
  },
  "homepage": "https://github.com/TradaTech/sunseed#readme",
  "dependencies": {
    "@babel/core": "7.4.0",
    "@babel/parser": "7.4.2",
    "@babel/plugin-transform-flow-strip-types": "7.4.0",
    "@babel/runtime": "7.4.3",
    "@babel/template": "7.4.0",
    "@babel/traverse": "7.4.3",
    "@babel/types": "7.4.0",
    "axios": "0.19.0",
    "node-fetch": "2.6.0",
    "prettier": "1.17.1",
    "terser": "4.0.0",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "husky": "^2.3.0",
    "jest": "24.7.1",
    "moment": "^2.24.0",
    "ms": "^2.1.2",
    "standard": "^12.0.1"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist/*"
    ],
    "env": [
      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*",
      "!<rootDir>/node_modules/"
    ]
  }
}
